<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="StudentMapper">
    <select id="getStudent" parameterType="int" resultType="role">
            select id,role_name as roleNamenote from t_role where id = #{id}
               </select>
</mapper>
